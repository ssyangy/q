<?xml version="1.0" encoding="GB2312"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="categorys">
	<typeAlias alias="category" type="q.domain.Category" />
	<resultMap id="categoryResult" class="category">
		<result property="id" column="id" />
        <result property="name" column="name" />
        <result property="intro" column="intro" />
        <result property="status" column="status" />
        <result property="created" column="created" />
		<result property="modified" column="modified" />
	</resultMap>

	<sql id="category.columns">
	 id,
	 name,
	 intro,
	 status,
	 created,
	 modified
	</sql>

	<select id="selectCategoryById" resultMap="categoryResult">
		select
		<include refid="category.columns" />
		from
		q.category
		where id=#value#
	</select>

	<insert id="insertCategory">
		insert into q.category
		(
		<include refid="category.columns" />
		) values (
		#id#,
		#name#,
		#intro#,
		#status#,
		#created#,
		#modified#		
		)
	</insert>
 
    <select id="selectCategorys" resultMap="categoryResult">
		select
		*
		from
		q.category
	</select>

	<!--<update id="IbatisItemVerticalPropertyDAO.update" parameterClass="itemVerticalProperty"> 
		update auction_vertical_property set AUCTION_VAR_ID=#itemIdStr#, VERTICAL_TYPE=#verticalType#, 
		PROPERTIES=#propertyList,handler=com.taobao.item.dao.ibatis.support.ItemVerticalPropertiesTypeHandlerCallback#, 
		GMT_MODIFIED=sysdate where id = #id# </update> -->
</sqlMap>